{
	"name": "htswap",
	"type": "module",
	"version": "0.1.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/ahme-dev/htswap.git"
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist",
		"dist-min",
		"dist-compat"
	],
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"module": "./dist/index.js",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"scripts": {
		"build": "tsdown",
		"test": "vitest run",
		"prepublishOnly": "pnpm run build",
		"fix": "biome check --write"
	},
	"devDependencies": {
		"@biomejs/biome": "2.0.6",
		"@happy-dom/global-registrator": "^18.0.1",
		"typescript": "^5.8.3",
		"vitest": "^3.2.4",
		"tsdown": "^0.14.1"
	}
}
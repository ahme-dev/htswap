{
	"name": "htswap",
	"type": "module",
	"version": "0.3.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ahme-dev/htswap.git"
	},
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist",
		"dist-min",
		"dist-compat"
	],
	"main": "./dist/htswap.cjs",
	"types": "./dist/htswap.d.ts",
	"module": "./dist/htswap.js",
	"exports": {
		".": {
			"import": {
				"types": "./dist/htswap.d.ts",
				"default": "./dist/htswap.js"
			},
			"require": {
				"types": "./dist/htswap.d.cts",
				"default": "./dist/htswap.cjs"
			}
		}
	},
	"scripts": {
		"build": "tsdown",
		"test": "vitest run --dangerouslyIgnoreUnhandledErrors",
		"prepublishOnly": "pnpm run build",
		"fix": "biome check --write"
	},
	"devDependencies": {
		"@biomejs/biome": "2.0.6",
		"jsdom": "^26.1.0",
		"publint": "^0.3.12",
		"tsdown": "^0.14.1",
		"typescript": "^5.8.3",
		"vitest": "^3.2.4"
	}
}